<robot name="alvar_tile" xmlns:xacro="http://ros.org/wiki/xacro">

  <link name="alvar_tile">
    <visual>
    <geometry>
      <mesh filename="$(find larics_gazebo_worlds)/models/crack_2/alvar_tile.dae" scale="0.3 0.3 0.3" />
    </geometry>
    </visual>
    <inertial>
      <inertia ixx="10" ixy="0.0" ixz="0.0" iyy="10" iyz="0.0"
        izz="10" />
    <mass value="1.0" />
    </inertial>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="$(find larics_gazebo_worlds)/models/crack_2/alvar_tile.dae" scale="0.3 0.3 0.3" />
      </geometry>
    </collision>
  </link>

  <gazebo>
    <static>true</static>
  </gazebo>

</robot>
